{
	"name": "be-theme-app",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --port=4800",
		"copy:theme-assets": "cpy ./projects/be-theme/assets/* ./dist/be-theme/assets",
		"copy:theme-styles": "cpy ./projects/be-theme/src/lib/core/styles ./dist/be-theme/lib/core/styles",
		"build": "ng build be-theme && npm run copy:theme-assets && npm run copy:theme-styles",
		"build:theme": "ng build be-theme && npm run build:theme-scss",
		"build:theme-scss": "scss-bundle -c projects/be-theme/scss-bundle.config.json",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~10.1.1",
		"@angular/cdk": "^10.2.3",
		"@angular/common": "~10.1.1",
		"@angular/compiler": "~10.1.1",
		"@angular/core": "~10.1.1",
		"@angular/flex-layout": "^10.0.0-beta.32",
		"@angular/forms": "~10.1.1",
		"@angular/material": "~10.2.2-sha-00cc53cd1",
		"@angular/platform-browser": "~10.1.1",
		"@angular/platform-browser-dynamic": "~10.1.1",
		"@angular/router": "~10.1.1",
		"@ngx-translate/core": "^13.0.0",
		"@ngx-translate/http-loader": "^6.0.0",
		"rxjs": "~6.6.0",
		"scss-bundle": "^3.1.2",
		"tslib": "^2.0.0",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.1001.1",
		"@angular/cli": "~10.1.1",
		"@angular/compiler-cli": "~10.1.1",
		"@types/jasmine": "~3.5.0",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "^12.11.1",
		"codelyzer": "^6.0.0",
		"cpy-cli": "^3.1.1",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~5.0.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.2",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"ng-packagr": "^10.1.0",
		"protractor": "~7.0.0",
		"ts-node": "~8.3.0",
		"tslint": "~6.1.0",
		"typescript": "~4.0.2"
	}
}
